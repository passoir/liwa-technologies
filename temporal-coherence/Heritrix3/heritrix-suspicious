echo start Heritrix
CLASSPATH=`echo $(ls lib/*.jar)":heritrix-3.0.0/bin" | sed -e 's/ /:/g'`
I=0
COUNT=1
END=$(wc -l seeds/suspicious| cut -f1 -d\ )
while   [ $I -le 10 ]
do
java -Xmx1024M -XX:+UseConcMarkSweepGC -XX:+CMSClassUnloadingEnabled -classpath $CLASSPATH -Drobots.start=$I -Drobots.count=$COUNT org.liwa.coherence.HeritrixLauncher -a liwa -p 8845
I=$(( $I + $COUNT) % 7)
echo $START
done
echo end